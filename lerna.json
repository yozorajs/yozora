{
  "version": "2.0.1",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "graphType": "all",
      "verifyAccess": false,
      "ignoreChanges": ["*.md", "*.txt", "test/**", "**/test**/"],
      "message": ":bookmark:  chore(release): publish %s",
      "push": false,
      "private": false,
      "forcePublish": true
    },
    "version": {
      "allowBranch": "main",
      "forcePublish": true
    }
  }
}
