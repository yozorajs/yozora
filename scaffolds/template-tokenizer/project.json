{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "template-tokenizer",
  "sourceRoot": "scaffolds/jest-for-tokenizer/src",
  "projectType": "library",
  "tags": ["other"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "scaffolds/template-tokenizer",
        "commands": [
          "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.mjs --rootDir ."
        ]
      },
      "configurations": {
        "coverage": {
          "commands": [
            "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.mjs --rootDir . --coverage"
          ]
        },
        "update": {
          "commands": [
            "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.mjs --rootDir . -u"
          ]
        }
      }
    }
  }
}
