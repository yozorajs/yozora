{
  "name": "@yozora/parser-gfm-ex",
  "version": "2.3.14",
  "description": "A markdown parser with built-in tokenizers to fully support GFM and GFM extensions",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/yozora.git",
    "directory": "packages/parser-gfm-ex"
  },
  "homepage": "https://github.com/yozorajs/yozora/tree/v2.3.13/packages/parser-gfm-ex#readme",
  "keywords": [
    "markdown",
    "markdown ast",
    "markdown parser",
    "gfm",
    "gfm extensions"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c ../../rollup.config.mjs",
    "clean": "rimraf lib",
    "test": "vitest run --config ../../vitest.config.ts",
    "test:coverage": "vitest run --config ../../vitest.config.ts --coverage",
    "test:update": "vitest run --config ../../vitest.config.ts -u"
  },
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.cjs"
  },
  "source": "./src/index.ts",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "license": "MIT",
  "files": [
    "lib/",
    "!lib/**/*.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@yozora/ast": "workspace:^",
    "@yozora/core-parser": "workspace:^",
    "@yozora/tokenizer-autolink": "workspace:^",
    "@yozora/tokenizer-autolink-extension": "workspace:^",
    "@yozora/tokenizer-blockquote": "workspace:^",
    "@yozora/tokenizer-break": "workspace:^",
    "@yozora/tokenizer-definition": "workspace:^",
    "@yozora/tokenizer-delete": "workspace:^",
    "@yozora/tokenizer-emphasis": "workspace:^",
    "@yozora/tokenizer-fenced-code": "workspace:^",
    "@yozora/tokenizer-heading": "workspace:^",
    "@yozora/tokenizer-html-block": "workspace:^",
    "@yozora/tokenizer-html-inline": "workspace:^",
    "@yozora/tokenizer-image": "workspace:^",
    "@yozora/tokenizer-image-reference": "workspace:^",
    "@yozora/tokenizer-indented-code": "workspace:^",
    "@yozora/tokenizer-inline-code": "workspace:^",
    "@yozora/tokenizer-link": "workspace:^",
    "@yozora/tokenizer-link-reference": "workspace:^",
    "@yozora/tokenizer-list": "workspace:^",
    "@yozora/tokenizer-paragraph": "workspace:^",
    "@yozora/tokenizer-setext-heading": "workspace:^",
    "@yozora/tokenizer-table": "workspace:^",
    "@yozora/tokenizer-text": "workspace:^",
    "@yozora/tokenizer-thematic-break": "workspace:^"
  }
}
