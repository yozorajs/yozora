{
  "name": "@yozora/parser",
  "version": "1.0.0",
  "description": "A markdown parser with rich built-in tokenizers",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/yozora.git",
    "directory": "packages/parser"
  },
  "homepage": "https://yozora.guanghechen.com/docs/package/parser",
  "keywords": [
    "markdown",
    "markdown ast",
    "markdown parser",
    "gfm",
    "gfm extensions"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/ && tsc -p tsconfig.src.json --emitDeclarationOnly",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir ."
  },
  "dependencies": {
    "@yozora/ast": "^1.0.0",
    "@yozora/core-parser": "^1.0.0",
    "@yozora/tokenizer-admonition": "^1.0.0",
    "@yozora/tokenizer-autolink": "^1.0.0",
    "@yozora/tokenizer-autolink-extension": "^1.0.0",
    "@yozora/tokenizer-blockquote": "^1.0.0",
    "@yozora/tokenizer-break": "^1.0.0",
    "@yozora/tokenizer-definition": "^1.0.0",
    "@yozora/tokenizer-delete": "^1.0.0",
    "@yozora/tokenizer-emphasis": "^1.0.0",
    "@yozora/tokenizer-fenced-code": "^1.0.0",
    "@yozora/tokenizer-footnote": "^1.0.0",
    "@yozora/tokenizer-footnote-definition": "^1.0.0",
    "@yozora/tokenizer-footnote-reference": "^1.0.0",
    "@yozora/tokenizer-heading": "^1.0.0",
    "@yozora/tokenizer-html-block": "^1.0.0",
    "@yozora/tokenizer-html-inline": "^1.0.0",
    "@yozora/tokenizer-image": "^1.0.0",
    "@yozora/tokenizer-image-reference": "^1.0.0",
    "@yozora/tokenizer-indented-code": "^1.0.0",
    "@yozora/tokenizer-inline-code": "^1.0.0",
    "@yozora/tokenizer-inline-math": "^1.0.0",
    "@yozora/tokenizer-link": "^1.0.0",
    "@yozora/tokenizer-link-reference": "^1.0.0",
    "@yozora/tokenizer-list": "^1.0.0",
    "@yozora/tokenizer-list-item": "^1.0.0",
    "@yozora/tokenizer-math": "^1.0.0",
    "@yozora/tokenizer-paragraph": "^1.0.0",
    "@yozora/tokenizer-setext-heading": "^1.0.0",
    "@yozora/tokenizer-table": "^1.0.0",
    "@yozora/tokenizer-text": "^1.0.0",
    "@yozora/tokenizer-thematic-break": "^1.0.0"
  }
}
