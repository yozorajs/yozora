{
  "name": "@yozora/parser",
  "version": "2.1.5",
  "description": "A markdown parser with rich built-in tokenizers",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/yozora.git",
    "directory": "packages/parser"
  },
  "homepage": "https://github.com/yozorajs/yozora/tree/v2.1.5/packages/parser#readme",
  "keywords": [
    "markdown",
    "markdown ast",
    "markdown parser",
    "gfm",
    "gfm extensions"
  ],
  "type": "module",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.cjs"
  },
  "source": "./src/index.ts",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib/ && cross-env NODE_ENV=production rollup -c ../../rollup.config.mjs",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.mjs --rootDir .",
    "test:update": "yarn test -u"
  },
  "dependencies": {
    "@yozora/ast": "^2.1.5",
    "@yozora/core-parser": "^2.1.5",
    "@yozora/tokenizer-admonition": "^2.1.5",
    "@yozora/tokenizer-autolink": "^2.1.5",
    "@yozora/tokenizer-autolink-extension": "^2.1.5",
    "@yozora/tokenizer-blockquote": "^2.1.5",
    "@yozora/tokenizer-break": "^2.1.5",
    "@yozora/tokenizer-definition": "^2.1.5",
    "@yozora/tokenizer-delete": "^2.1.5",
    "@yozora/tokenizer-ecma-import": "^2.1.5",
    "@yozora/tokenizer-emphasis": "^2.1.5",
    "@yozora/tokenizer-fenced-code": "^2.1.5",
    "@yozora/tokenizer-footnote": "^2.1.5",
    "@yozora/tokenizer-footnote-definition": "^2.1.5",
    "@yozora/tokenizer-footnote-reference": "^2.1.5",
    "@yozora/tokenizer-heading": "^2.1.5",
    "@yozora/tokenizer-html-block": "^2.1.5",
    "@yozora/tokenizer-html-inline": "^2.1.5",
    "@yozora/tokenizer-image": "^2.1.5",
    "@yozora/tokenizer-image-reference": "^2.1.5",
    "@yozora/tokenizer-indented-code": "^2.1.5",
    "@yozora/tokenizer-inline-code": "^2.1.5",
    "@yozora/tokenizer-inline-math": "^2.1.5",
    "@yozora/tokenizer-link": "^2.1.5",
    "@yozora/tokenizer-link-reference": "^2.1.5",
    "@yozora/tokenizer-list": "^2.1.5",
    "@yozora/tokenizer-math": "^2.1.5",
    "@yozora/tokenizer-paragraph": "^2.1.5",
    "@yozora/tokenizer-setext-heading": "^2.1.5",
    "@yozora/tokenizer-table": "^2.1.5",
    "@yozora/tokenizer-text": "^2.1.5",
    "@yozora/tokenizer-thematic-break": "^2.1.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.6"
  }
}
